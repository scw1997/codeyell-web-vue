    "test": "lint-staged",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.{css,less,scss,json,html,md}": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scw1997/swico.git"
  },
  "keywords": [
    "swico",
    "typescript",
    "vue",
    "javascript",
    "rspack"
  ],
  "author": "scw",
  "license": "ISC",
  "dependencies": {
    "vue": "~3.5.23",
    "swico": "2.6.0-beta.10",
    "@ant-design/icons-vue": "^7.0.1",
    "@kangc/v-md-editor": "^2.3.18",
    "ali-oss": "^6.17.1",
    "ant-design-vue": "^4.0.8",
    "axios": "^1.6.3",
    "dayjs": "^1.11.7",
    "highlight.js": "^11.7.0",
    "marked": "^4.2.12",
    "monaco-editor": "^0.34.1",
    "pinia": "^2.1.7"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "autoprefixer": "10.3.7",
