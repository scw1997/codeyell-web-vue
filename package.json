    "tailwindcss": "^4.1.7",
    "@types/node": "22.14.1",
    "globals": "16.0.0",
    "eslint": "9.24.0",
    "eslint-plugin-vue": "10.0.0",
    "vue-eslint-parser": "10.1.3",
    "typescript-eslint": "8.30.1",
    "@eslint/js": "9.24.0",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "cross-env": "7.0.3",
    "husky": "9.0.10",
    "@commitlint/cli": "18.5.0",
    "@commitlint/config-conventional": "18.5.0",
    "lint-staged": "15.2.1"
